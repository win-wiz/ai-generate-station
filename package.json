{
  "name": "ai-generate-station",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "commit": "node scripts/auto-commit.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:setup": "npx tsx scripts/db-setup.ts",
    "db:reset": "rm -f db.sqlite && pnpm db:setup",
    "d1:deploy": "npx tsx scripts/d1-deploy.ts",
    "d1:migrate": "wrangler d1 migrations apply ai-generate-station-db",
    "d1:studio": "wrangler d1 execute ai-generate-station-db --command='SELECT name FROM sqlite_master WHERE type=\"table\";'",
    "dev": "next dev --turbo --port 3000",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "build:cf": "next build",
    "pages:build": "npx @cloudflare/next-on-pages@1",
    "pages:deploy": "pnpm build:cf && pnpm pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "preview:cf": "pnpm build:cf && wrangler pages dev .vercel/output/static --compatibility-date=2024-01-15",
    "cf:build": "pnpm install && pnpm build:cf && pnpm pages:build",
    "cf:verify": "npx tsx scripts/verify-deployment.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.41.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "15.5.2",
    "next-auth": "5.0.0-beta.25",
    "postgres": "^3.4.7",
    "puppeteer": "^24.22.0",
    "rate-limiter-flexible": "^7.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.41.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vercel": "^47.0.4",
    "wrangler": "^3.78.12"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.14.0"
}
